{
  "name": "Template_Bun_Elysia",
  "version": "1.0.50",
  "scripts": {
    "test": "vitest --ui",
    "test:once": "vitest run",
    "dev": "nodemon --ext ts --exec \"tsc && node dist/index.js\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "pg-promise": "^11.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.19",
    "@vitest/ui": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "latest",
    "vitest": "latest"
  }
}
